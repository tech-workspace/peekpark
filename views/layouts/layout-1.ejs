  <script src="/js/util.js"></script>
  <script src="/js/main.js"></script>

  <script>
    window.onload = function clock() {
      var d = new Date();
      var date = d.getUTCDate();
      var month = d.getUTCMonth();
      var year = d.getUTCFullYear();
      var monthArr = [
        "January",
        "February",
        "March",
        "April",
        "May",
        "June",
        "July",
        "August",
        "September",
        "October",
        "November",
        "December",
      ];
      month = monthArr[month];
      document.getElementById("spanSysDate").innerHTML =
        date + " " + month + " " + year;
      document.getElementById("spanSysYear").innerHTML = year;
    };
  </script>
  <!-- End Scripts -->
</html>
